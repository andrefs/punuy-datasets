{
  "name": "punuy-datasets",
  "version": "5.5.2",
  "description": "",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./datasets/*": "./src/datasets/*.ts",
    "./lib/*": "./src/lib/*.ts"
  },
  "scripts": {
    "build": "./src/scripts/compile-schema",
    "preversion": "ts-node src/scripts/update-readme.ts && npm run lint",
    "postversion": "git push && git push --tags && gh release create v$npm_package_version --title v$npm_package_version --generate-notes",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "NODE_OPTIONS=--max-old-space-size=4096 prettier --write \"**/*.+(js|ts|json)\"",
    "prepare": "npm run build",
    "postinstall": "patch-package",
    "test": "./src/scripts/validate"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andrefs/punuy-datasets.git"
  },
  "keywords": [
    "semantic",
    "measures",
    "semantic",
    "similarity",
    "evaluation",
    "gold",
    "standard",
    "dataset"
  ],
  "author": "Andr√© Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrefs/punuy-datasets/issues"
  },
  "homepage": "https://github.com/andrefs/punuy-datasets#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "ajv": "8.12.0",
    "ajv-cli": "5.0.0",
    "ajv-formats": "2.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript-json-schema": "0.63.0"
  },
  "dependencies": {
    "patch-package": "8.0.0"
  }
}
